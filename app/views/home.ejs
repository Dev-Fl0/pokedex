<%- include('partials/header') %>

<main class="main">

    <% pokemons.forEach(pokemon => { %>

        <div class="pokemon_div">
            <h3 class="pokemon_title"><%= pokemon.name.fr %> #<%= pokemon.pokedexId %> </h3>
            <img src="<%= pokemon.sprites.regular %>" alt="pokemon.name.fr" class="pokemon_img_front">
            <div class="div_button">
                <button class="shiny_button">Shiny</button>
                <button class="info_button">Infos</button>
            </div>
        </div>
     
    <% }) %>

</main>

